{{ define "mood_view" }}
{{/* Created on 26. 03. 2021 */}}
{{/* Time-stamp: <2022-04-01 18:19:29 krylon> */}}
<table class="table">
  <thead>
    <tr>
      <th>Time</th>
      <th>Score</th>
      <th>Note</th>
    </tr>
  </thead>

  <tbody>
    {{ range .Mood }}
    <tr>
      <td>{{ fmt_time .Timestamp }}</td>
      <td>{{ .Score }}</td>
      <td>{{ .Note }}</td>
    </tr>
    {{ end }}
  </tbody>
</table>
{{ end }}
